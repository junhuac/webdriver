package com.webdriver.qa.automation.ams.enums;

/**
 * @author bhutton(brianhutton@cinchcast.com)
 * Created on: 04/06/2012
 *
 * Enum for representation of Content Category Option in drop down.
 */
public enum ContentCategory
{
  NO_CATEGORY("0"),
  ARTS("1"),
  AUTOMOTIVE("70"),
  BUSINESS("4"),
  COMEDY("51"),
  EDUCATION("10"),
  ENTERTAINMENT("12"),
  GAMES_AND_HOBBIES("24"),
  HEALTH("23"),
  KIDS_AND_FAMILIES("31"),
  MUSIC("Music"),
  NEWS_AND_POLITICS("32"),
  POLITICS("35"),
  POLITICS_SUSTAINABILITY_AND_ACTIVISM("220"),
  RELIGION_AND_SPIRITUALITY("37"),
  SCIENCE_AND_MEDICINE("40"),
  SOCIETY_AND_CULTURE("6"),
  SPORTS_AND_RECREATION("43"),
  TECHNOLOGY("44"),
  TRAVEL("45"),
  TV_AND_FILMS("14");

  /**
   * Label for drop-down option
   */
  private String value;

  /**
   * Constructor for this enum<br>
   *
   * @param value The label shown in the drop-down
   */
  ContentCategory(String value)
  {
    this.value = value;
  }

  /**
   * @return The label shown in the drop-down
   */
  public String getValue()
  {
    return value;
  }
}

package com.webdriver.qa.automation.ams.enums;

/**
 * @author bhutton(brianhutton@cinchcast.com)
 * Created on: 15/06/2012
 *
 * Enum that represents the different options for Field Restrictions in Registration Forms
 */
public enum FieldRestrictions {
  NO("no)"),
  ALPHANUMERIC("alphanumeric"),
  LETTERSWITHBASICPUNC("letterswithbasicpunc"),
  LETTERSONLY("lettersonly");

  /**
   * Value as seen in the code.
   */
  private String value;

  /**
   * Constructor for the enum.
   *
   * @param value As seen in the code.
   */
  FieldRestrictions(String value) {
    this.value = value;
  }

  /**
   * Retrieves the value as seen in the code.
   *
   * @return {@link String} representation of the value.
   */
  public String getValue() {
    return value;
  }
}
